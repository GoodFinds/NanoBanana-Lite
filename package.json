{
  "name": "nano-img-hono",
  "version": "1.0.0",
  "description": "AI 图像生成抽卡游戏 - 基于 Hono.js 和 Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --port 8791",
    "dev:local": "wrangler dev --local --port 8791",
    "deploy": "wrangler deploy",
    "build": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,css,html}\"",
    "lint": "eslint src --ext .ts,.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "image-generation",
    "hono",
    "cloudflare-workers",
    "typescript",
    "card-game"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.89.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251225.0",
    "@types/node": "^22.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
